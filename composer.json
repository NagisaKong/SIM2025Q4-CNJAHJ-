{
  "name": "csit314/csr-platform",
  "description": "CSR platform structured by modules without framework",
  "type": "project",
  "require": {
    "php": "^8.2",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "fakerphp/faker": "^1.21"
  },
  "autoload": {
    "psr-4": {
      "CSRPlatform\\": "csit314-csr-platform/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CSRPlatform\\Tests\\": "csit314-csr-platform/src/tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
